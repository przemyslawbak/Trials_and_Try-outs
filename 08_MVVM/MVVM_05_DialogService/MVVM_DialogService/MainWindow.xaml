<Window x:Class="MVVM_DialogService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewmodels="clr-namespace:MVVM_DialogService.ViewModels"
        xmlns:local="clr-namespace:MVVM_DialogService"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <viewmodels:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <StackPanel Margin="100">
            <Button Content="Yes No Dialog" Command="{Binding YesNoCommand}"/>
            <Button Content="Alert Dialog" Command="{Binding AlertCommand}"/>
        </StackPanel>
    </Grid>
</Window>
