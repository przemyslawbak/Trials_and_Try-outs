{
  "Data": {
    "WorldCities": {
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=WorldCities;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "Logging": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "AllowedHosts": "*",
    "StaticFiles": {
      "Headers": {
        "Cache-Control": "no-cache, no-store",
        "Pragma": "no-cache",
        "Expires": "-1"
      }
    }
  }
}
