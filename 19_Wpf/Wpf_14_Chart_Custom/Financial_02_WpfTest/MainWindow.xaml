<Window x:Class="Financial_02_WpfTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Financial_02_WpfTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <StackPanel>
        <Polyline Height="300" Width="300"  Name="_myPolyline" Stroke="{Binding ColorName}" StrokeThickness="1" Points="{Binding Points}" ></Polyline>
    </StackPanel>
</Window>
