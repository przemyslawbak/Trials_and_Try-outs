<!DOCTYPE html>
<html>
<head>
    <title>Aplikacja</title>
    <meta charset="utf-8">
</head>
<body class="container">
    <div>
        TEST
    </div>
    <script>
        console.log(typeof 4.5)
        // → number
        console.log(typeof "x")
        // → string
        console.log(- (10 - 2))
        //-8
        console.log(3 > 2)
        // → true
        console.log(3 < 2)
// → false
        console.log("Aardvark" < "Zoroaster")
// → true
        console.log("Itchy" != "Scratchy")
        // → true
        console.log("Apple" == "Orange")
// → false
        console.log(NaN == NaN)
// → false
        console.log(false || true)
        // → true
        console.log(false || false)
// → false
        console.log(true ? 1 : 2);
        // → 1
        console.log(false ? 1 : 2);
// → 2
        console.log(8 * null)
        // → 0
        console.log("5" - 1)
        // → 4
        console.log("5" + 1)
        // → 51
        console.log("five" * 2)
        // → NaN
        console.log(false == 0)
// → true
        console.log(null == undefined);
        // → true
        console.log(null == 0);
// → false
        console.log(null || "user")
        // → user
        console.log("Agnes" || "user")
// → Agnes
    </script>
</body>
</html>